generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id    String @id @default(uuid())

  firstName String? @map("first_name")
  lastName  String? @map("last_name")
  imageUrl String? @map("image_url")
}
